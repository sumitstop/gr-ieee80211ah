<?xml version="1.0"?>
<block>
  <name>OFDM Mapper AH</name>
  <key>ieee80211ah_ah_ofdm_mapper</key>
  <category>[IEEE802.11 AH]</category>
  <import>import ieee80211ah</import>
  <make>ieee80211ah.ah_ofdm_mapper($encoding, $debug)</make>
  <callback>set_encoding_ah($encoding)</callback>
  <param>
		<name>Encoding AH</name>
		<key>encoding</key>
		<value>BPSK_1_2</value>
		<type>int</type>	
		<option>
			<name>BPSK 1/2</name>
			<key>BPSK_1_2</key>
		</option>
		<option>
			<name>QPSK 1/2</name>
			<key>ieee80211ah.QPSK_1_2</key>
		</option>
		<option>
			<name>QPSK 3/4</name>
			<key>ieee80211ah.QPSK_3_4</key>
		</option>
		<option>
			<name>16QAM 1/2</name>
			<key>ieee80211ah.QAM16_1_2</key>
		</option>
		<option>
			<name>16QAM 3/4</name>
			<key>ieee80211ah.QAM16_3_4</key>
		</option>
		<option>
			<name>64QAM 2/3</name>
			<key>ieee80211ah.QAM64_2_3</key>
		</option>
		<option>
			<name>64QAM 3/4</name>
			<key>ieee80211ah.QAM64_3_4</key>
		</option>
		<option>
			<name>64QAM 5/6</name>
			<key>ieee80211ah.QAM64_5_6</key>
		</option>
		<option>
			<name>256QAM 3/4</name>
			<key>ieee80211ah.QAM256_3_4</key>
		</option>
	</param>
	<param>
		<name>Debug</name>
		<key>debug</key>
		<value>False</value>
		<type>bool</type>
		<option>
			<name>Enable</name>
			<key>True</key>
		</option>
		<option>
			<name>Disable</name>
			<key>False</key>
		</option>
	</param>
	<check>$encoding in range(9)</check>
	<sink>
		<name>in</name>
		<type>message</type>
	</sink>
	<source>
		<name>out</name>
		<type>byte</type>
		<nports>1</nports>
	</source>
	
</block>

 